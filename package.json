{
  "name": "boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn index.ts",
    "build": "node index.js",
    "lint:check": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "prettier:check": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "lint:fix": "eslint . --fix",
    "prettier:fix": "prettier --write .",
    "lint-prettier-fix": "yarn lint:fix && yarn prettier:fix",
    "lint-prettier-check": "yarn lint:check && yarn prettier:check",
    "lint-prettier": "yarn lint-prettier-fix && yarn lint-prettier-check",
    "prod": "NODE_ENV=production nodemon index.js"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "connect-mongo": "^5.0.0",
    "consola": "^3.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb": "^5.4.0",
    "mongoose": "^7.1.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  }
}
